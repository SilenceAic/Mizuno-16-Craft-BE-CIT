{
  "format_version": "1.21.110",
  "minecraft:block": {
    "description": {
      "identifier": "cit:pink_carpet_0",
      "menu_category": {
        "category": "construction",
        "group": "minecraft:itemGroup.name.woolCarpet"
      },
      "traits": {
        "minecraft:placement_position": {
          "enabled_states": ["minecraft:block_face"]
        }
      },
      "states": {
        "cit:rotation": {
          "values": { "min": 0, "max": 15 }
        },
        "cit:dynamic_state": {
          "values": { "min": 0, "max": 1 }
        }
      }
    },
    "components": {
      "minecraft:geometry": {
        "identifier": "geometry.cit.template_carpet_0",
        "bone_visibility": {
          "up_0": "q.block_state('minecraft:block_face') != 'up' && math.mod(q.block_state('cit:rotation'), 4) == 0",
          "up_22_5": "q.block_state('minecraft:block_face') != 'up' && math.mod(q.block_state('cit:rotation'), 4) == 1",
          "up_45": "q.block_state('minecraft:block_face') != 'up' && math.mod(q.block_state('cit:rotation'), 4) == 2",
          "up_67_5": "q.block_state('minecraft:block_face') != 'up' && math.mod(q.block_state('cit:rotation'), 4) == 3",
          "inventory": "q.block_state('minecraft:block_face') == 'bottom'"
        }
      },
      "minecraft:collision_box": {
        "origin": [-8.5, 9, -2],
        "size": [16, 5, 9]
      },
      "minecraft:selection_box": {
        "origin": [-8.5, 9, -2],
        "size": [16, 5, 9]
      },
      "minecraft:material_instances": {
        "*": {
          "texture": "pink_wool",
          "render_method": "alpha_test",
          "ambient_occlusion": true,
          "face_dimming": true
        },
        "_planks": {
          "texture": "dark_oak_planks",
          "render_method": "alpha_test",
          "ambient_occlusion": true,
          "face_dimming": true
        }
      },
      "minecraft:placement_filter": {
        "conditions": [
          {
            "allowed_faces": ["east", "south", "west", "north"]
          }
        ]
      },
      "cit:rotation": {},
      "cit:dynamic_state": {}
    },
    "permutations": [
      {
        "condition": "q.block_state('cit:rotation') >= 4 || q.block_state('minecraft:block_face') == 'east'",
        "components": {
          "minecraft:transformation": { "rotation": [0, -90, 0] }
        }
      },
      {
        "condition": "q.block_state('cit:rotation') >= 8 || q.block_state('minecraft:block_face') == 'south'",
        "components": {
          "minecraft:transformation": { "rotation": [0, 180, 0] }
        }
      },
      {
        "condition": "q.block_state('cit:rotation') >= 12 || q.block_state('minecraft:block_face') == 'west'",
        "components": {
          "minecraft:transformation": { "rotation": [0, 90, 0] }
        }
      },
      {
        "condition": "q.block_state('cit:dynamic_state') == 1",
        "components": {
          "minecraft:material_instances": {
            "*": {
              "texture": "pink_wool",
              "render_method": "alpha_test",
              "ambient_occlusion": true,
              "face_dimming": true
            },
            "white_wool": {
              "texture": "white_wool",
              "render_method": "alpha_test",
              "ambient_occlusion": true,
              "face_dimming": true
            },
            "_planks": {
              "texture": "dark_oak_planks",
              "render_method": "alpha_test",
              "ambient_occlusion": true,
              "face_dimming": true
            }
          }
        }
      }
    ]
  }
}
