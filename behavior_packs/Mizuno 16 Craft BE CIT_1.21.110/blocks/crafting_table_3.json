{
  "format_version": "1.21.110",
  "minecraft:block": {
    "description": {
      "identifier": "cit:crafting_table_3",
      "menu_category": {
        "category": "nature"
      },
      "traits": {
        "minecraft:placement_position": {
          "enabled_states": ["minecraft:block_face"]
        }
      },
      "states": {
        "cit:rotation": {
          "values": { "min": 0, "max": 15 }
        },
        "cit:dynamic_state": {
          "values": { "min": 0, "max": 1 }
        }
      }
    },
    "components": {
      "minecraft:geometry": {
        "identifier": "geometry.cit.crafting_table_3"
      },
      "minecraft:collision_box": {
        "origin": [-8, 0, 7],
        "size": [16, 16, 1]
      },
      "minecraft:selection_box": {
        "origin": [-8, 0, 7],
        "size": [16, 16, 1]
      },
      "minecraft:material_instances": {
        "*": {
          "texture": "crafting_table_2",
          "render_method": "alpha_test",
          "ambient_occlusion": true,
          "face_dimming": true
        },
        "tool": {
          "texture": "crafting_table_tool",
          "render_method": "alpha_test",
          "ambient_occlusion": true,
          "face_dimming": true
        }
      },
      "minecraft:placement_filter": {
        "conditions": [
          {
            "allowed_faces": ["side"]
          }
        ]
      },
      "cit:rotation": {},
      "cit:dynamic_state": {}
    },
    "permutations": [
      {
        "condition": "q.block_state('cit:dynamic_state') == 1",
        "components": {
          "minecraft:geometry": {
            "identifier": "geometry.cit.crafting_table_3a"
          },
          "minecraft:collision_box": {
            "origin": [-8, 0, 7],
            "size": [16, 16, 1]
          },
          "minecraft:selection_box": {
            "origin": [-8, 0, 7],
            "size": [16, 16, 1]
          },
          "minecraft:material_instances": {
            "*": {
              "texture": "oak_planks",
              "render_method": "alpha_test",
              "ambient_occlusion": true,
              "face_dimming": true
            },
            "tool": {
              "texture": "crafting_table_toola",
              "render_method": "alpha_test",
              "ambient_occlusion": true,
              "face_dimming": true
            }
          }
        }
      },
      {
        "condition": "q.block_state('cit:rotation') >= 4 || q.block_state('minecraft:block_face') == 'east'",
        "components": {
          "minecraft:transformation": { "rotation": [0, -90, 0] }
        }
      },
      {
        "condition": "q.block_state('cit:rotation') >= 8 || q.block_state('minecraft:block_face') == 'south'",
        "components": {
          "minecraft:transformation": { "rotation": [0, 180, 0] }
        }
      },
      {
        "condition": "q.block_state('cit:rotation') >= 12 || q.block_state('minecraft:block_face') == 'west'",
        "components": {
          "minecraft:transformation": { "rotation": [0, 90, 0] }
        }
      }
    ]
  }
}
